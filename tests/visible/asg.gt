# Testing Goat assignments

proc main()
    int a;
    bool b;
begin
    a := 42;
    write a;
    write "\n";
    a := a;
    write a;
    write "\n";
    a := (4*5*6) + (1*2*3);
    write a;
    write "\n";
    a := (4/5/6) - (1/2/3);
