proc main()
	int a[3,4];
	int i;
	int j;
begin
	i := 0;
	j := 0;
	while i < 3 do
		while j < 4 do
			a[i,j] := i * 2 + j;
			j := j+1;
		od
		i := i+1;
	od
	call writeresult (2, 2);
end

proc writeresult(val int x, val int y)
	int i;
	int j;
begin
	i := 0;
	j := 0;
	while i < x do
		while j < y do
			write a[i,j];
			write " ";
			j := j+1;
		od
		write "\n";
		i := i+1;
	od
end